{
  "name": "coach",
  "version": "1.0.0",
  "description": "WhatsApp Coach API using Express, Twilio, OpenAI, and Supabase",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "vercel-build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-cron": "^4.2.1",
    "openai": "^4.58.1",
    "twilio": "^4.21.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
